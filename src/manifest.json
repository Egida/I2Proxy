{
  "manifest_version": 3,
  "name": "I2Proxy",
  "version": "1.3",
  "description": "An extension for connecting to I2P network while randomizing browser fingerprint",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "proxy",
    "storage",
    "tabs",
    "scripting",
    "cookies"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "i2p-proxy@nemesis",
      "strict_min_version": "91.1.0"
    }
  },
  "background": {
    "scripts": ["background.js"]
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "icon/icon.png"
    }
  },
  "icons": {
    "48": "icon/icon.png"
  },
  "host_permissions": [
    "<all_urls>"
  ]
}
